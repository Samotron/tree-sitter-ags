{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ags",
      "camelcase": "Ags",
      "title": "Ags",
      "scope": "source.ags",
      "file-types": [
        "ags"
      ],
      "injection-regex": "^ags$",
      "class-name": "TreeSitterAgs",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parse AGS Files",
    "authors": [
      {
        "name": "Sam Rogers",
        "email": "james.s.rogers23@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-ags"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
